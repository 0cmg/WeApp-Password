<view class="page">
  <view class="page__hd">
    <view class="page__title">场景管理</view>
  </view>
  <view class="page__bd">
    <view class="weui-cells__title">我添加的场景</view>
    <block wx:for="{{servers}}">
      <navigator url="../info/info?id={{item.key}}">
        <view class="weui-cells">
          <view class="weui-form-preview">
            <view class="weui-form-preview__hd">
              <view class="codeStyle">{{item.code}}</view>
            </view>
            <view class="weui-form-preview__bd">
              <view class="weui-form-preview__item">
                {{item.name}}({{item.username}})
              </view>
            </view>
          </view>
        <progress percent="{{item.time}}" stroke-width='4' />
        </view>
      </navigator>
    </block>
  </view>
</view>
<image src="data:image/png;base64,"
catchtap="scanCode" class="footBtn"></image>