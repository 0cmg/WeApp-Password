<view class="page">
  <view class="page__hd">
    <view class="page__title">场景</view>
  </view>
  <view class="page__bd">
    <view class="weui-cells__title">我的场景</view>
    <!-- 使用block循环数据 -->
    <block wx:for="{{servers}}">
    <!-- 使用navigator来进行跳转，类似A标签 -->
      <navigator url="../info/info?id={{item.key}}">
        <view class="weui-cells">
          <view class="weui-form-preview">
            <view class="weui-form-preview__hd">
              <view class="codeStyle">{{item.code}}</view>
            </view>
            <view class="weui-form-preview__bd">
              <view class="weui-form-preview__item">
                {{item.name}} （{{item.username}}）
              </view>
            </view>
          </view>
        <progress percent="{{timer}}" stroke-width='4' />
        </view>
      </navigator>
    </block>
    <block wx:if="{{servers.length == 0}}">
    <view style="text-align:center;padding-top:70px;">
    
    <image src="https://ypingcn.github.io/assets/imgs/404.png" style="width:150px;height:150px;"></image>
    <view  class="tips">尚无任何场景，请点击右下角扫描按钮添加</view>
    
    </view>
    </block>
  </view>
</view>
<!-- 页面右下角按钮，使用base64解码  -->
<image src="data:image/png;base64," catchtap="scanCode" class="footBtn"></image>